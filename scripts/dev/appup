#!/bin/bash

# GS-CMS Application Startup Shortcut
# This shortcut executes the full app startup script

# Get the directory where this script is located (project root)
SCRIPT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

# Execute the app-up script from the scripts directory
exec "$SCRIPT_DIR/scripts/app-up.sh" "$@"